                string perInputparameters = context.InputParameters["usuarioid"].ToString();
                string cliInputparameters = context.InputParameters["clienteid"].ToString();
                string nameInputparameters = context.InputParameters["nombreCliente"].ToString();
                string visualizoInputparameters = context.InputParameters["texto"].ToString();
                string optionInputparameters = context.InputParameters["tipoOption"].ToString();

                // The InputParameters collection contains all the data passed in the message request.  

                cxm_mx_hst_per_fichacliente dataFC = _processor.SetInfoEjecutivoAFichaCliente(Guid.Parse(perInputparameters), Guid.Parse(cliInputparameters), nameInputparameters, visualizoInputparameters, optionInputparameters);
                cxm_mx_mst_acc_interaccion dataIN = _processor.SetInteraccion(nameInputparameters, Guid.Parse(cliInputparameters));
