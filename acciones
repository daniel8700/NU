// ObtÃ©n el valor actual de sessionStorage y parsea como objeto
var tutelaObj = JSON.parse(sessionStorage.getItem('cxm_tutela')) || {};

// Actualiza el valor de "tutela" a true
tutelaObj.tutela = true;

// Almacena el objeto actualizado en sessionStorage
sessionStorage.setItem('cxm_tutela', JSON.stringify(tutelaObj));

// Verifica el resultado
console.log(sessionStorage.getItem('cxm_tutela'));
