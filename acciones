object perInputObject = context.InputParameters["usuarioid"];
object cliInputObject = context.InputParameters["clienteid"];
object nameInputObject = context.InputParameters["nombreCliente"];
object visualizoInputObject = context.InputParameters["texto"];
object optionInputObject = context.InputParameters["tipoOption"];

// Validar y convertir solo si los valores no son nulos
string perInputparameters = perInputObject != null ? perInputObject.ToString() : string.Empty;
string cliInputparameters = cliInputObject != null ? cliInputObject.ToString() : string.Empty;
string nameInputparameters = nameInputObject != null ? nameInputObject.ToString() : string.Empty;
string visualizoInputparameters = visualizoInputObject != null ? visualizoInputObject.ToString() : string.Empty;
string optionInputparameters = optionInputObject != null ? optionInputObject.ToString() : string.Empty;

// Proceder con las conversiones y el procesamiento
cxm_mx_hst_per_fichacliente dataFC = _processor.SetInfoEjecutivoAFichaCliente(
    Guid.Parse(perInputparameters),
    Guid.Parse(cliInputparameters),
    nameInputparameters,
    visualizoInputparameters,
    optionInputparameters
);

cxm_mx_mst_acc_interaccion dataIN = _processor.SetInteraccion(nameInputparameters, Guid.Parse(cliInputparameters));
